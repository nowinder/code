   "source": [
    "# 不递归的删除指定文件夹下后缀文件\n",
    "import os\n",
    "path = '/data/caizhizheng/2D/v2/'\n",
    "file = os.listdir(path)\n",
    "for file in file:\n",
    "    file_raw = path + file\n",
    "    if os.path.isfile(file_raw):\n",
    "        if file.endswith('.tif'):\n",
    "            os.remove(file_raw)"
   ]
  }
